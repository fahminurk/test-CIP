// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./../test.db"
}

model suplier {
  id_suplier Int @id @default(autoincrement())
  nama_suplier String
  alamat String
  email String
  produk produk[]
}

model produk {
  id Int @id @default(autoincrement())
  nama String
  deskripsi String
  harga Int
  stok Int
  foto String
  suplier_id Int @unique
  suplier suplier @relation(fields: [suplier_id], references: [id_suplier])
}